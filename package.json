{
  "name": "review-master",
  "version": "0.1.2",
  "description": "A library to visualize business intelligence derived from sentiment analysis on product reviews",
  "author": {
    "name": "Bora Elci",
    "email": "bora.elci@columbia.edu"
  },
  "license": "MIT",
  "repository": "boraelci/review-master",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">= 16 <20"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "scripts": {
    "build": "rollup -c",
    "start": "concurrently \"rollup -c -w\" \"cd example && npm start\"",
    "prepare": "run-s build",
    "test": "react-scripts test --watchAll=false --coverage --env=jsdom",
    "lint": "eslint src --ext .ts --ext .tsx",
    "prettier": "prettier --config .prettierrc --write .",
    "docs:predeploy": "cd website && npm install && npm run build",
    "docs:deploy": "gh-pages -d website/build",
    "docs": "run-s docs:predeploy docs:deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "peerDependencies": {
    "bootstrap": "^5.2.3",
    "chart.js": "^4.2.1",
    "react": "^16.0.0",
    "react-bootstrap": "^2.7.4",
    "react-chartjs-2": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44",
    "@typescript-eslint/parser": "^5.44",
    "babel-eslint": "^10.0.3",
    "chart.js": "^4.2.1",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^8.28",
    "eslint-config-prettier": "^8.5",
    "eslint-plugin-jest": "^27.1",
    "gh-pages": "^2.2.0",
    "jest": "^29.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^16.0.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "~29.0",
    "typescript": "~4.9"
  },
  "files": [
    "dist"
  ]
}
